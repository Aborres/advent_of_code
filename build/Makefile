ifndef config
  config=debug64
endif
export config

PROJECTS := Puzzles

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

Puzzles: 
	@echo "==== Building Puzzles ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f Puzzles.make

clean:
	@${MAKE} --no-print-directory -C . -f Puzzles.make clean

